{
	"name": "caspr",
	"license": "MIT",
	"displayName": "Caspr",
	"description": "",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.caspr.red",
		"onCommand:extension.caspr.yellow",
		"onCommand:extension.caspr.reset",
		"onCommand:extension.caspr.test"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.caspr.red",
				"title": "Caspr: Red Alert"
			},
			{
				"command": "extension.caspr.yellow",
				"title": "Caspr: Yellow Alert"
			},
			{
				"command": "extension.caspr.reset",
				"title": "Caspr: reset config"
			},
			{
				"command": "extension.caspr.test",
				"title": "Caspr: test"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "5.2.6",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.34"
	},
	"dependencies": {
		"@types/graphql": "^14.0.7",
		"apollo-cache-inmemory": "^1.4.3",
		"apollo-client": "^2.4.13",
		"apollo-link-context": "^1.0.14",
		"apollo-link-http": "^1.5.11",
		"apollo-link-ws": "^1.0.14",
		"graphql": "^14.1.1",
		"graphql-tag": "^2.10.1",
		"node-fetch": "^2.3.0",
		"subscriptions-transport-ws": "^0.9.15",
		"ws": "^6.1.4"
	}
}
